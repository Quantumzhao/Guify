<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="urn:Guify.Namespace" xmlns="urn:Guify.Namespace" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="./Group.xsd" />
    <xs:include schemaLocation="./YesNo.xsd" />
    <xs:include schemaLocation="./SelectFolder.xsd" />
    <xs:include schemaLocation="./Container.xsd" />
    <xs:element name="Verb" type="VerbType" />
